        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UNIVERSIDAD MARIANA - SAN JUAN DE PASTO"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
